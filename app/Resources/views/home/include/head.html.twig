{% extends 'index/include/head.html.twig' %}

{% block frontend_index_head_css %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/frontend/css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/frontend/css/freelancer.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/frontend/font-awesome/css/font-awesome.min.css') }}"
{% endblock %}

{% block frontend_index_head_js %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/frontend/js/jquery.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/frontend/js/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/frontend/js/jquery.nouislider.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/frontend/js/freelancer.js') }}"></script>
{% endblock %}

{% block frontend_index_head_fonts %}
    {{ parent() }}
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Montserrat:400,700">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic">
{% endblock %}
